<odoo>

   <record id="calatayud_product_template_product_form" model="ir.ui.view">
        <field name="name">calatayud.product.template.product.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view" />
        <field name="arch" type="xml">
           <xpath
                expr="//field[@name='taxes_id']"
                position="before">
                <field name="sale_only_piece"/>
            </xpath>
        </field>
    </record>

</odoo>

